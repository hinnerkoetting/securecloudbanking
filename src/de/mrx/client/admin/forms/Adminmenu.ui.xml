<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.menuButton {
			width:100%;
			padding: 5px;
			margin-bottom: 5px;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel>
			<g:Button styleName="{style.menuButton}" ui:field="accounts" />
			<g:Button styleName="{style.menuButton}" ui:field="externalBanks" />
			<g:Button styleName="{style.menuButton}" ui:field="newTransaction" />
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 
