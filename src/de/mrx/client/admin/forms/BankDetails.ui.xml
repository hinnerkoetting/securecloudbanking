<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="../../SCB.css" type='de.mrx.client.admin.forms.BankDetails.MyStyle'>
		.active {
			color:blue
		}
		.nonActive {
			color:black
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Label ui:field="title" styleName="{style.title}" />
		<g:Label ui:field="subTitle" styleName="{style.subtitle}" />
		<g:HorizontalPanel>
			<g:Button ui:field="enableDisplayAccounts"/>
			<g:Button ui:field="enableEdit"/>
			<g:Button ui:field="delete"/>
		</g:HorizontalPanel>
		<g:SimplePanel ui:field="content"/>
		
		
	</g:HTMLPanel>
</ui:UiBinder> 