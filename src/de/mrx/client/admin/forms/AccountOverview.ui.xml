<!DOCTYPE ui:UiBinder SYSTEM 
"http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="../../SCB.css">
		.searchDiv {
			background-color:#DDDDDD;
			position:relative;			
			margin-top:50px; 
		}
		.tableDiv {
				position:relative;
				margin-top:20px;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Label styleName="{style.title}" ui:field="title"/>
		<div>
			<div class="{style.searchDiv}">
				<table>
					<tr>
						<td><g:Label ui:field="descOwner"/></td>
						<td><g:TextBox ui:field ="searchOwner"/></td>
						<td><g:Label ui:field="descAccountNr"/></td>
						<td><g:TextBox ui:field ="searchAccountNr"/></td>
						<td><g:Button ui:field ="search"/></td>
					</tr>
				</table>
		
			</div>
			<div>
				<g:FlexTable styleName="{style.tableDiv}" ui:field="overviewTable">
				
				</g:FlexTable>
			</div>
			
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 