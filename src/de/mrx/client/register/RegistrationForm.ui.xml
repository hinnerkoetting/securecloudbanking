<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'
ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
    ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
    ui:generateLocales="default">
   <g:HTMLPanel> 
    
    <table border="1">
    <tr>
    	<td> <ui:msg description="regLblLastName">Last Name</ui:msg></td>
    	<td> <g:TextBox ui:field='lastName'/></td>
    	<td> <ui:msg description="regLblFirstName">First Name</ui:msg></td>
    	<td> <g:TextBox ui:field='firstName'/></td>
    	<td> <ui:msg description="regLblEmail">Email (only Googlemail)</ui:msg></td>
    	<td> <g:TextBox ui:field='email'/></td>
    </tr>
    <tr>
    	
    	<td> <ui:msg description="street">Street</ui:msg></td>
    	<td> <g:TextBox ui:field='street'/></td>
    	<td> <ui:msg description="houseNr">House Nr</ui:msg></td>
    	<td> <g:TextBox ui:field='houseNr'/></td>
    	
    </tr>
    	<td> <ui:msg description="plz">Postal Code</ui:msg></td>
    	<td> <g:TextBox ui:field='plz'/></td>
    	<td> <ui:msg description="city">City</ui:msg></td>
    	<td> <g:TextBox ui:field='city'/></td>
    	<td> <g:ListBox ui:field='language' visibleItemCount='1'/></td>
    <tr>
    </tr>

    <tr>
    	<td><g:CheckBox ui:field='agb'><ui:msg description="agbhint">I accept the Terms Of Services</ui:msg></g:CheckBox></td>
    	<td><g:Anchor ui:field="agblink"><ui:msg description="agbhint">ToS</ui:msg></g:Anchor></td>
    	<td> <g:Button></g:Button> </td>
    </tr>
    </table> 
    
  </g:HTMLPanel>
</ui:UiBinder>
		
		